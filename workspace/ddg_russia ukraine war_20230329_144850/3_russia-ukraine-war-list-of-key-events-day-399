.test(window.OnetrustActiveGroups)?0:1,usrPersonaAds=isNonPersonalizedAds}</script><script>function getGAEventData ( category, label, position, suffix ) {
			return {
				event_category: category + ' - ' + suffix + ' ' + position,
				event_label: label,
			};
		}

		function setupEventListener( item, data ) {
			item.addEventListener("click", function() {
				//TODO: Data here is captured in closure, it won't reflect the latest data
				// use a function to get the latest data every time a user clicks
				window.gtag( 'event', 'Click', data );
			});
		}

		function keepReadingGAList ( list ) {
			for(var i=0; i < list.length; i++){
				setupEventListener( list[i], getGAEventData( 'Keep Reading', list[i].innerText, i + 1, 'URL' ) );
			};
		}
		function explainerGAList ( list ) {
			const category = 'Explainer Widget';
			for(var i=0; i < list.length - 1; i++){
				setupEventListener( list[i], getGAEventData( category, list[i].getAttribute('data-original-title'), i + 1, 'URL' ) );
			};
			const lastIndex = list.length - 1;
			if(lastIndex > 0) {
				setupEventListener( list[lastIndex], getGAEventData( category, list[lastIndex].innerText, '', 'Full Coverage' ) );
			}
		}

		var metadataCheckInterval;
		var intervalCounter;
		var maxIntervalCounter = 200;
		var keepReadingList = document.getElementsByClassName('more-on__link');
		var explainerList = document.getElementsByClassName('explainer-widget__link');
		//funtion name has been preserved to be called by global router component
		function handleEventsAndAdsLoad() {
			intervalCounter = 0;
			clearInterval(metadataCheckInterval);
			metadataCheckInterval = setInterval( function() {
				intervalCounter += 1;
				if (intervalCounter >= maxIntervalCounter) {
					clearInterval(metadataCheckInterval);
				} else if ( keepReadingList.length > 0 ) {
					clearInterval(metadataCheckInterval);
					keepReadingGAList(keepReadingList);
				} else if ( explainerList.length > 0 ) {
					clearInterval(metadataCheckInterval);
					explainerGAList(explainerList);
				}
			}, 500 );
		};
